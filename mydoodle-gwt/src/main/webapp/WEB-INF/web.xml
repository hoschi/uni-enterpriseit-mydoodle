<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>

	<!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>welcomeGWT.html</welcome-file>
	</welcome-file-list>

	<!-- servlets -->
	<servlet>
		<servlet-name>PollServiceGwtImpl</servlet-name>
		<servlet-class>
    de.uniluebeck.itm.ep5.mydoodle.gwt.server.PollServiceGwtImpl
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PollServiceGwtImpl</servlet-name>
		<url-pattern>/de.uniluebeck.itm.ep5.mydoodle.gwt.Main/poll</url-pattern>
	</servlet-mapping>
</web-app>